{
  "name": "project_evoting",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "jest --config ../../../tests/jest.config.js --selectProjects backend",
    "test:watch": "jest --config ../../../tests/jest.config.js --selectProjects backend --watch",
    "test:coverage": "jest --config ../../../tests/jest.config.js --selectProjects backend --coverage",
    "test:ci": "jest --config ../../../tests/jest.config.js --selectProjects backend --coverage --watchAll=false"
  },
  "author": "RAAGINEE",
  "license": "ISC",
  "dependencies": {
    "archiver": "^7.0.1",
    "async": "^3.2.6",
    "bcrypt": "^5.1.1",
    "body-parser": "^1.20.3",
    "child_process": "^1.0.2",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^4.18.2",
    "json-bigint": "^1.0.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.0.3",
    "multer": "^1.4.5-lts.1",
    "os": "^0.1.2",
    "pdf-parse": "^1.1.1",
    "pdf-parser": "^1.0.5",
    "python-shell": "^5.0.0",
    "uuid": "^9.0.1",
    "ws": "^8.18.1"
  },
  "devDependencies": {
    "jest": "^29.7.0",
    "supertest": "^6.3.3"
  },
  "jest": {
    "testEnvironment": "node",
    "testMatch": ["**/__tests__/**/*.js", "**/?(*.)+(spec|test).js"],
    "collectCoverageFrom": [
      "**/*.js",
      "!**/node_modules/**",
      "!**/coverage/**",
      "!**/__tests__/**",
      "!jest.config.js",
      "!**/Lib/**"
    ],
    "coverageDirectory": "coverage",
    "coverageReporters": ["text", "lcov", "html", "json"],
    "coverageThreshold": {
      "global": {
        "branches": 70,
        "functions": 70,
        "lines": 70,
        "statements": 70
      }
    }
  }
}
